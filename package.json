{
  "name": "codeandglory",
  "version": "1.0.1-0",
  "description": "Static web for codeandglory.be",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node index.js",
    "postversion": "git push",
    "heroku-push": "git push heroku master",
    "patch-release": "nvm version patch",
    "minor-release": "nvm version minor",
    "major-release": "nvm version major",
    "watch-sass": "npm run compile-sass -- --watch",
    "compile-sass": "node-sass  --output-style compressed --ident-type space --output ./css ./sass",
    "build": "npm run compile-sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh33dafi/codeandglory.git"
  },
  "author": "Yannick Houbrix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sh33dafi/codeandglory/issues"
  },
  "homepage": "https://github.com/sh33dafi/codeandglory#readme",
  "dependencies": {
    "node-static": "^0.7.7"
  },
  "devDependencies": {
    "node-sass": "^3.4.2"
  }
}
